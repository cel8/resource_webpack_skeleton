{
  "env": {
      "browser": true,
      "es2021": true,
      "jest": true
  },
  "extends": [
    "airbnb-base",
    "prettier"
  ],
  "overrides": [
  ],
  "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
  },
  "rules": {
    "no-console": "off",
    "no-param-reassign": ["error", { "props": false }]
  },
  "settings": {
    "import/resolver": {
     "webpack": {
       "config": "webpack.config.js"
     }
    }
  }
}
